{
  "name": "land-of-oolymer",
  "private": true,
  "scripts": {
    "browser": "npm run serve & npm run watch",
    "build": "./node_modules/.bin/polymer build",
    "serve": "./node_modules/.bin/polymer serve --port 9090",
    "test": "./node_modules/.bin/polymer test --local chrome,firefox",
    "watch": "./node_modules/.bin/browser-sync start --port 3000 --proxy 'localhost:9090' --no-ui --files '*.html, demo/**/*.html, src/**/*.html, test/**/*.html' --startPath \"/components/$npm_package_name/\""
  },
  "devDependencies": {
    "bower": "1.8.2",
    "browser-sync": "2.18.13",
    "polymer-cli": "1.5.7",
    "web-component-tester": "6.4.1"
  }
}
